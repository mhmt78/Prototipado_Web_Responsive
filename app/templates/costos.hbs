<div class="row">
	<div class="col s12">
		<div class="card-panel">
			<div class="card-content">
				<div class="row">
					<h5 class="col s12">Determinacion de costos</h5>
				</div>				
				<div class="row">
					<form class="col s12">
						<div class="row">
							<div class="input-field col s12 m6 l6">
					          <input id="Materia" type="text" class="validate">
					          <label for="Materia">Materiales</label>
					        </div>					      
						</div>
						<div class="row">
							<div class="input-field col s12 m4 l4">
					          <input id="cant" type="number" class="validate">
					          <label for="cant">Cantidad</label>
					        </div>
							<div class="input-field col s12 m4 l4">
					          <input id="compra" type="text" class="validate">
					          <label for="compra">Unidad</label>
					        </div>
					        <div class="input-field col s12 m4 l4">
					          <input id="costo" type="number" class="validate">
					          <label for="costo">Precio por Unidad</label>
					        </div>	
					        				
						</div>
						<div class="row">							
					        <div class="input-field col s12 m4 l4">
								<select>
									  <option value="" disabled selected>Elegir</option>
									  <option value="1">Manual</option>
									  <option value="2">Maquina</option>
								</select>
								<label> Tipo de Produccion</label>
							 </div>
							<div class="input-field col s12 m4 l4">
								<input id="totalcant" type="number" class="validate">
								<label for="totalcant">Precio Total</label>
					        </div>
						    <div class="col s12 m3 l3 offset-m1 offset-l1">
					        	<button class="btn waves-effect waves-light grey" id="btnEnviar" type="submit" name="action">
					        		Enviar
   								 	<i class="material-icons right">send</i>
  								</button>
					        </div>
						</div>
					</form>
				</div>
			</div>
		</div>
	</div>
</div>